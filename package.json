{
  "name": "cdm",
  "version": "3.0.0-beta.4",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint",
    "commit": "git-cz",
    "release:beta": "standard-version -p beta -r patch",
    "report": "vue-cli-service build --report",
    "serve:n1": "vue-cli-service serve --mode n1 --port 8001",
    "serve:n2": "vue-cli-service serve --mode n2 --port 8002",
    "serve:n3": "vue-cli-service serve --mode n3 --port 8003",
    "serve:n4": "vue-cli-service serve --mode n4 --port 8004",
    "serve:n5": "vue-cli-service serve --mode n5 --port 8005",
    "serve:preview": "vue-cli-service serve --mode preview  --port 8000"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^1.0.7",
    "@antv/g2": "^4.0.15",
    "@antv/g6": "^4.0.3",
    "@vue/composition-api": "^1.0.0-beta.22",
    "ant-design-vue": "^1.7.2",
    "axios": "^0.21.0",
    "core-js": "^3.1.2",
    "enquire.js": "^2.1.6",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "mockjs2": "1.0.8",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "sortablejs": "^1.12.0",
    "store": "^2.0.12",
    "vue": "^2.6.12",
    "vue-i18n": "^8.17.4",
    "vue-native-websocket": "^2.0.14",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1",
    "xterm": "^4.9.0",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-fit": "^0.4.0",
    "xterm-addon-search": "^0.7.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.2.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@vue/cli-plugin-babel": "^4.0.4",
    "@vue/cli-plugin-eslint": "^4.0.4",
    "@vue/cli-plugin-router": "^4.0.4",
    "@vue/cli-plugin-unit-jest": "^4.5.9",
    "@vue/cli-plugin-vuex": "^4.0.4",
    "@vue/cli-service": "^4.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.2",
    "compression-webpack-plugin": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "git-revision-webpack-plugin": "^3.0.6",
    "husky": "^4.3.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "standard-version": "^9.0.0",
    "style-resources-loader": "^1.3.2",
    "vue-cli-plugin-style-resources-loader": "~0.1.4",
    "vue-template-compiler": "^2.6.12",
    "webpack-theme-color-replacer": "^1.3.12"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
